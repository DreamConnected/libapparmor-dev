
#include <tunables/global>

@{SNAP_NAME}="ubuntu-image"
@{SNAP_REVISION}="95"
@{PROFILE_DBUS}="snap_2eubuntu_2dimage_2eubuntu_2dimage"
@{INSTALL_DIR}="/{,var/lib/snapd/}snap"

profile "snap.ubuntu-image.ubuntu-image" (attach_disconnected,mediate_deleted,complain) {
  # set file rules so that exec() inherits our profile unless there is
  # already a profile for it (eg, snap-confine)
  / rwkl,
  /** rwlkm,
  /** pix,

  capability,
  change_profile,
  dbus,
  network,
  mount,
  remount,
  umount,
  pivot_root,
  ptrace,
  signal,
  unix,


}
