include <tunables/global>

/usr/sbin/bonnie++ flags=(enforce) {
  include <abstractions/base>

  capability setgid,
  capability setuid,

  /etc/passwd r,
  /etc/nsswitch.conf r,

  /**/Bonnie.* wr,
  /**/Bonnie.*/ wr,

  /**/[0-9][0-9][0-9][0-9][0-9][0-9][0-9]*  wr,
  /**/*[0-9][0-9][0-9][0-9][0-9][0-9][0-9]  wr,
  /**/*[0-9][0-9][0-9][0-9][0-9][0-9][0-9]??  wr,
}
